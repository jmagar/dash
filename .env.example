# API Configuration
REACT_APP_API_URL=http://localhost:4000
REACT_APP_WS_URL=ws://localhost:4000

# Authentication
REACT_APP_JWT_STORAGE_KEY=ssh_token
REACT_APP_AUTH_PERSIST=true

# Feature Flags
REACT_APP_ENABLE_MFA=true
REACT_APP_ENABLE_DOCKER=true
REACT_APP_ENABLE_PACKAGES=true

# UI Configuration
REACT_APP_DEFAULT_THEME=system
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_TERMINAL_FONT=monospace
REACT_APP_TERMINAL_FONT_SIZE=14

# WebSocket Configuration
REACT_APP_WS_RECONNECT_ATTEMPTS=5
REACT_APP_WS_RECONNECT_INTERVAL=2000

# Error Reporting
REACT_APP_ERROR_REPORTING=true
REACT_APP_ERROR_LOGGING_LEVEL=error

# Performance
REACT_APP_CACHE_TTL=3600
REACT_APP_DEBOUNCE_DELAY=300
REACT_APP_THROTTLE_DELAY=1000

# Development
REACT_APP_DEBUG=false
REACT_APP_MOCK_API=false
REACT_APP_API_TIMEOUT=30000

# Docker Configuration
REACT_APP_DOCKER_REFRESH_INTERVAL=5000
REACT_APP_DOCKER_STATS_INTERVAL=2000

# File Explorer
REACT_APP_MAX_UPLOAD_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=*

  "scripts": {
    "start": "node dist/server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run type:watch\"",
    "dev:client": "TSCONFIG=tsconfig.client.json craco start",
    "dev:server": "ts-node-dev --project tsconfig.server.json --respawn --transpile-only src/server/server.ts",
    "build": "npm run clean && npm run type:check && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production TSCONFIG=tsconfig.client.json craco build",
    "build:server": "NODE_ENV=production tsc -p tsconfig.server.json",
    "clean": "rimraf dist .tsbuildinfo tsconfig.*.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint:fix": "eslint src --ext .ts,.tsx --fix --cache",
    "type:check": "tsc --noEmit -p tsconfig.json && npm run type:check:client && npm run type:check:server",
    "type:check:client": "tsc --noEmit -p tsconfig.client.json",
    "type:check:server": "tsc --noEmit -p tsconfig.server.json",
    "type:watch": "tsc --noEmit --watch -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate",
    "fix-types": "ts-node --project tsconfig.server.json scripts/safe-type-fixes.ts",
    "fix-types:dry": "ts-node --project tsconfig.server.json scripts/safe-type-fixes.ts --dry-run",
    "fix-types:test": "ts-node --project tsconfig.server.json scripts/safe-type-fixes.ts src/shared/dtos/base/__tests__",
    "check": "tsc -b && eslint . --ext .ts,.tsx --max-warnings 0"
  },
  "browserslist": {
    "production": [

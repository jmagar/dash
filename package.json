    "axios": "^1.6.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dockerode": "^4.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "monaco-editor": "^0.52.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "^5.0.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",

    "xterm": "^5.3.0"
  },
  "scripts": {
    "start": "node dist/server/server.js",
    "clean": "rm -rf dist build",
    "build": "REACT_APP_DISABLE_AUTH=true SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:all": "npm run clean && npm run build && npm run build:server",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "PORT=4000 REACT_APP_DISABLE_AUTH=true REACT_APP_WDS_SOCKET_PORT=0 react-scripts start",
    "dev:server": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server/server.ts",
    "test": "react-scripts test",
    "test:server": "jest",
    "migrate": "ts-node scripts/migrate.ts",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.18.61",
    "@types/pg": "^8.11.10",
    "@types/ssh2": "^1.14.0",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.2.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "web-vitals": "^2.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
